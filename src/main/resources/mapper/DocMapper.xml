<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dmsserver.mapper.DocMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.dmsserver.entity.Doc">
        <id column="id" property="id" />
        <result column="ebookId" property="ebookId" />
        <result column="name" property="name" />
        <result column="sort" property="sort" />
        <result column="viewCount" property="viewCount" />
        <result column="voteCount" property="voteCount" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, ebookId, name, sort, viewCount, voteCount
    </sql>

</mapper>
